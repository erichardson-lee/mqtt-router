{
  "name": "@erichardson-lee/mqtt-router",
  "packageManager": "yarn@3.1.1",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "files": [
    "dist/**/*",
    "package.json"
  ],
  "scripts": {
    "build": "yarn build:cjs & yarn build:esm & yarn build:types & echo \"Building Project\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "ava"
  },
  "devDependencies": {
    "@tsconfig/esm": "^1.0.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/mqtt": "^2.5.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "ava": "^4.3.1",
    "mqtt-pattern": "^1.2.0",
    "ts-node": "^10.9.1"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}